@using Entities.Concrete
@model List<Talent>
@{
    ViewBag.Title = "TalentCard";
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>ŞEYMA ÇELEBİ</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Skills Profile Widget template Responsive, Login form web template,Flat Pricing tables,Flat Drop downs  Sign up Web Templates, Flat Web Templates, Login sign up Responsive web template, SmartPhone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--Custom Theme files-->
    <link href="~/AdminLTE-3.0.4/web/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <!--//Custom Theme files -->
    <!--web font-->
    <link href='//fonts.googleapis.com/css?family=Questrial' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic' rel='stylesheet' type='text/css'>
    <!--//web font-->
    <!--circle-chart-->
    <script src="js/jquery-1.11.1.min.js"></script>
    <!--//circle-chart-->
    <!--ResponsiveTabs-->
    <script src="~/AdminLTE-3.0.4/web/js/jquery.circlechart.js"></script>
    <script src="~/AdminLTE-3.0.4/web/js/jquery-1.11.1.min.js"></script>
    <script src="~/AdminLTE-3.0.4/web/js/easyResponsiveTabs.js" type="text/javascript"></script>
    <script type="text/javascript">
            $(document).ready(function () {
                $('#horizontalTab').easyResponsiveTabs({
                    type: 'default', //Types: default, vertical, accordion
                    width: 'auto', //auto or any width like 600px
                    fit: true   // 100% fit in a container
                });
            });
    </script>
    <!--//ResponsiveTabs-->
</head>
<body>
    <!-- main -->
    <div class="main">
        <h1>Şeyma Çelebi Yetenek Sayfası</h1>
        <div class="main-info w3l">
            <div class="main-row">
              
                <!-- main-row-one -->
                <div class="profile-grid logo wthree">

                    
                    <img src="~/AdminLTE-3.0.4/web/images/5.jpg" alt="SEYMACELEBI"  width="225"/>
                    <h2>ŞEYMA ÇELEBİ </h2>
                    <p>Bilgisayar Mühendisliği Öğrencisi</p>

                </div>
                <h3 class="title">Yetenekler</h3>
                <div class="col-md-6 skills-right bar_group">

                    @*<div class='bar_group__bar thin' label='C# &nbsp; 100%' value='200' ></div>
        <div class='bar_group__bar thin' label='Mvc &nbsp; 65%' value='120'></div>
        <div class='bar_group__bar thin' label='Mssql &nbsp; 60%' value='125'></div>
        <div class='bar_group__bar thin' label='Java &nbsp; 45%' value='112'></div>
        <div class='bar_group__bar thin' label='Angular &nbsp; 40%' value='100'></div>*@

                   
                    @foreach (var item in Model)
                    {
                        <div class='bar_group__bar thin' label='@item.TalentName <a href="/Talent/UpdateTalent/@item.TalentId" class="btn btn-info">Yeteneği Güncelle</a> &nbsp;&nbsp; @item.TalentName%' value='@item.Range'></div>
                    }
                </div>
                <!--bar-js-->
                <script src="~/AdminLTE-3.0.4/web/js/bars.js"></script>
                <div class="social-icons agileits">
                    <ul>


                        <li><a href="https://www.linkedin.com/in/seyma-celebi/" class="be"></a></li>

                    </ul>
                </div>
                <div class="clear"> </div>
            </div>
        </div>
        <!-- copyright -->
        <div class="copyright">
        </div>
        <!-- //copyright -->
    </div>
    <!-- main -->
</body>
</html>