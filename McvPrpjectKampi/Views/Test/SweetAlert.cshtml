
@{
    ViewBag.Title = "SweetAlert";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>SweetAlert</h2>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="~/Scripts/jquery-3.3.1.js"></script>
<button class="btn btn-primary" id="btn1">Buton 1</button>
<button class="btn btn-warning" id="btn2">Buton 2</button>
<button class="btn btn-danger" id="btn3">Buton 3</button>
<script>
    $('#btn1').click(function () {
        swal("Merhaba Mvc Proje Kampı")
    });
</script>
<script>
    $('#btn2').click(function () {
        swal("Merhaba Mvc Proje Kampı", "Ders 96")
    });
</script>
<script>
    $('#btn3').click(function () {
        swal("Tebrikler","Merhaba Mvc Proje Kampı", "Ders 96")
    });
</script>